    "mime-types": "^2.1.35",
    "moment": "^2.30.1",
    "multiconf": "^3.2.0",
    "pg": "^8.10.0",
    "sequelize": "^6.30.0",
    "sequelize-cli": "^6.6.0",
    "swagger-ui-express": "^5.0.1",
