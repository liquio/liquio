{
  "name": "liquio-id-api",
  "version": "0.1.0",
  "engines": {
    "node": "22"
  },
  "private": true,
  "license": "SEE LICENSE IN ../LICENSE.md",
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "start:prod": "node dist/index.js",
    "build": "tsc",
    "lint": "eslint -c eslint.config.js .",
    "lint:fix": "eslint -c eslint.config.js . --fix",
    "bundle": "node bundle.cjs",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:e2e": "jest --coverage --verbose",
    "migration-up": "sequelize db:migrate --migrations-path ./migrations --config ./migrations-config.js",
    "migration-create": "sequelize migration:create --config ./migrations-config.js --name ",
    "migration-generate": "sequelize-cli migration:generate --config ./migrations-config.js --name new-migration",
    "migration-down": "sequelize db:migrate:undo:all --config ./migrations-config.js",
    "migration-down-name": "sequelize db:migrate:undo --config ./migrations-config.js --name $name"
  },
  "dependencies": {
    "@node-oauth/oauth2-server": "^5.2.0",
    "asn1.js": "^5.4.1",
    "asn1js": "^3.0.7",
    "axios": "^1.8.4",
    "bcrypt": "^6.0.0",
    "body-parser": "^2.2.0",
    "compression": "^1.8.0",
    "connect-session-sequelize": "^7.1.7",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.6",
    "cyrillic-to-translit-js": "^3.2.1",
    "express": "^5.2.1",
    "express-http-proxy": "^2.1.1",
    "express-session": "^1.18.1",
    "express-validator": "^7.2.1",
    "gost89": "^0.1.11",
    "jksreader": "^1.0.0",
    "js-lzma": "^0.1.0",
    "jsonwebtoken": "^9.0.3",
    "ldapts": "^7.4.0",
    "lodash": "^4.17.21",
    "mappr": "^6.1.0",
    "nconf": "^0.13.0",
    "node-cache": "^4.2.1",
    "node-jose": "^2.2.0",
    "node-rsa": "^1.1.1",
    "node-schedule": "^2.1.1",
    "otpauth": "^9.4.0",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "passport-oauth2": "^1.8.0",
    "pg": "^8.14.1",
    "pkijs": "^3.2.5",
    "redis": "^5.10.0",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3",
    "swagger-ui-express": "^5.0.1",
    "typescript-eslint": "^8.56.0"
  },
  "devDependencies": {
    "@testcontainers/postgresql": "^10.24.2",
    "@testcontainers/redis": "^11.10.0",
    "@types/bcrypt": "^6.0.0",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/express-http-proxy": "^1.6.6",
    "@types/express-session": "^1.18.1",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/lodash": "^4.17.16",
    "@types/node": "^25.0.2",
    "@types/node-jose": "^1.1.13",
    "@types/node-rsa": "^1.1.4",
    "@types/node-schedule": "^2.1.7",
    "@types/passport": "^1.0.17",
    "@types/passport-local": "^1.0.38",
