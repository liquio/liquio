    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "jest": "^30.2.0",
    "nock": "^14.0.4",
    "portfinder": "^1.0.37",
    "prettier": "^3.5.3",
    "supertest": "^7.1.0",
    "testcontainers": "^11.10.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
