    "pg": "^8.18.0",
    "pg-hstore": "^2.3.3",
    "redis": "^5.11.0",
    "sequelize": "^6.29.0",
    "sequelize-cli": "^6.6.5",
    "xml2js": "^0.6.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "eslint": "^10.0.0",
    "eslint-plugin-jsdoc": "^62.5.4",
    "jest": "^30.2.0",
    "nock": "^14.0.11"
  },
  "overrides": {
