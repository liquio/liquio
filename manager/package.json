    "lint:fix": "eslint \"**/*.js\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "amqplib": "^0.10.9",
    "axios": "^1.13.5",
    "ccolor": "^1.3.1",
    "express": "^5.2.1",
